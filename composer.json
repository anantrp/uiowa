{
    "name": "uiowa/uiowa01",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0-only",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "collegiate": {
            "type": "path",
            "url": "docroot/profiles/custom/collegiate"
        },
        "collegiate_basic": {
            "type": "path",
            "url": "docroot/profiles/custom/collegiate_basic"
        },
        "uiowa_bar": {
            "type": "git",
            "url": "git@github.com:uiowa/uiowa_bar"
        },
        "uiowa_auth": {
            "type": "git",
            "url": "git@github.com:uiowa/uiowa_auth"
        },
        "uiowa_footer": {
            "type": "git",
            "url": "git@github.com:uiowa/uiowa_footer"
        }
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "github-protocols": [
            "https"
        ]
    },
    "require": {
        "php": ">=7.1",
        "acquia/blt": "^10.4",
        "acquia/drupal-spec-tool": "^2.0.0",
        "acquia/memcache-settings": "*",
        "drupal/acquia_connector": "^1.5.0",
        "drupal/acquia_purge": "^1.0-beta3",
        "drupal/address": "^1.7",
        "drupal/adminimal_theme": "^1.5",
        "drupal/administerusersbyrole": "3.0-alpha2",
        "drupal/better_normalizers": "^1.0@beta",
        "drupal/cog": "^1.0.0",
        "drupal/components": "^1.0",
        "drupal/config_split": "^1.4",
        "drupal/core": "8.7.7",
        "drupal/date_recur_modular": "^1.0",
        "drupal/features": "^3.8",
        "drupal/heading": "^1.4",
        "drupal/layout_builder_styles": "^1.0@beta",
        "drupal/lb_direct_add": "1.x-dev",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.4",
        "drupal/qa_accounts": "^1.0.0-alpha1",
        "drupal/seckit": "^1.0.0-alpha2",
        "drupal/shield": "^1.2.0",
        "drupal/stage_file_proxy": "^1.0@beta",
        "drupal/superfish": "^1.2",
        "drupal/twig_tweak": "^2.4",
        "drupal/webform": "^5.2",
        "uiowa/collegiate": "*",
        "uiowa/collegiate_basic": "dev-master",
        "uiowa/uiowa_auth": "^1.0",
        "uiowa/uiowa_bar": "^1.0",
        "uiowa/uiowa_footer": "^1.0"
    },
    "require-dev": {
        "acquia/blt-require-dev": "^10.0.0",
        "drupal/devel": "^2.1",
        "drupal/stage_file_proxy": "^1.0@beta",
        "geerlingguy/drupal-vm": "^5.0"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/layout_builder_styles": {
                "Section Dependency": "https://www.drupal.org/files/issues/2019-08-23/layout_builder_styles-3062261-%2310.patch",
                "Group Styles": "https://www.drupal.org/files/issues/2019-08-16/style_groups_for_layout_builder_styles-3075502.patch"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/uiowa/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/uiowa/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            }
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    }
}
