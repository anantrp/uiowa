{
    "name": "uiowa/uiowa01",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0-only",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "collegiate": {
            "type": "vcs",
            "url": "git@github.com:uiowa/collegiate",
            "no-api": true
        },
        "uiowa_bar": {
            "type": "git",
            "url": "git@github.com:uiowa/uiowa_bar"
        },
        "uiowa_auth": {
            "type": "git",
            "url": "git@github.com:uiowa/uiowa_auth"
        },
        "uiowa_footer": {
            "type": "git",
            "url": "git@github.com:uiowa/uiowa_footer"
        }
    },
    "autoload": {
        "psr-4": {
            "Example\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "acquia/blt": "^10.3",
        "acquia/drupal-spec-tool": "^2.0.0",
        "acquia/lightning": "^4.0.4",
        "drupal/acquia_connector": "^1.5.0",
        "drupal/acquia_purge": "^1.0-beta3",
        "drupal/adminimal_theme": "^1.3",
        "drupal/administerusersbyrole": "3.0-alpha2",
        "drupal/better_normalizers": "^1.0@beta",
        "drupal/cog": "^1.0.0",
        "drupal/components": "^1.0",
        "drupal/config_split": "^1.4",
        "drupal/core": "^8.7.1",
        "drupal/features": "^3.8.0",
        "drupal/memcache": "2.0-alpha7",
        "drupal/paragraphs": "^1.8",
        "drupal/qa_accounts": "^1.0.0-alpha1",
        "drupal/seckit": "^1.0.0-alpha2",
        "drupal/shield": "^1.2.0",
        "drupal/superfish": "^1.2",
        "drupal/webform": "^5.2",
        "php": ">=7.1",
        "uiowa/collegiate": "dev-master#0b65163",
        "uiowa/uiowa_auth": "^1.0",
        "uiowa/uiowa_bar": "^1.0",
        "uiowa/uiowa_footer": "^1.0"
    },
    "require-dev": {
        "acquia/blt-require-dev": "^10.0.0",
        "drupal/devel": "^2.0.0",
        "drupal/stage_file_proxy": "^1.0@beta",
        "geerlingguy/drupal-vm": "^5.0"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": [],
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/uiowa/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/uiowa/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            }
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    }
}
