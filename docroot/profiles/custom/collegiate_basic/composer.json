    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^6",
        "drupal/devel": "^1.2",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "mikey179/vfsStream": "^1.6",
        "behat/mink": "^1.7",
