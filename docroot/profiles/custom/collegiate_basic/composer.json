        "drupal/masquerade": "2.x-dev#2011b21",
        "drupal/media_entity_browser": "^2.0@alpha",
        "drupal/memcache": "^2.0",
        "drupal/metatag": "^1.5",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.1",
        "drupal/redirect": "^1.1",
        "drupal/schema_metatag": "^1.2",
        "drupal/simple_sitemap": "^2.12"
