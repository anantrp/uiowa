{
    "name": "uiowa/collegiate",
    "description": "UIowa Collegiate profile.",
    "type": "drupal-profile",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "uiowa_events": {
            "type": "git",
            "url": "git@github.com:uiowa/uiowa_events"
        }
    },
    "require": {
        "composer/installers": "^1.6",
        "cweagans/composer-patches": "^1.6",
        "drupal/acquia_connector": "^1.14",
        "drupal/acquia_purge": "^1.0@beta",
        "drupal/adminimal_theme": "^1.3",
        "drupal/better_normalizers": "^1.0@beta",
        "drupal/block_field": "^1.0@alpha",
        "drupal/components": "^1.0",
        "drupal/config_actions_provider": "^1.0@RC",
        "drupal/config_sync": "^2.0@beta",
        "drupal/config_update": "^1.6",
        "drupal/entity_browser": "^2.0@alpha",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/entity_reference_layout": "1.x-dev",
        "drupal/entity_reference_revisions": "^1.5",
        "drupal/features": "^3.8",
        "drupal/field_group": "^1.0",
        "drupal/focal_point": "1.x-dev",
