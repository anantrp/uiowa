{
    "name": "uiowa/collegiate",
    "type": "drupal-profile",
    "description": "UIowa collegiate install profile.",
    "license": "GPL-2.0-or-later",
    "require": {
        "ext-dom": "*",
        "cweagans/composer-patches": "^1.6.4",
        "drupal/adminimal_theme": "^1.5",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "drupal/components": "^1.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/noreqnewpass": "^1.1",
        "drupal/redirect": "^1.4"
    },
    "require-dev": {
        "drupal/drupal-extension": "^3.4",
        "drupal/facets": "^1.2",
        "drupal/media_entity_generic": "1.x-dev",
        "drupal/schema_metatag": "^1.3",
        "drush/drush": "^9.7",
        "webflo/drupal-core-require-dev": "*"
    },
    "config": {
        "platform": {
            "php": "7.0.8"
        },
        "preferred-install": {
            "drupal/core": "dist"
        }
    },
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@composer drupal-scaffold"
        ],
        "post-update-cmd": [
            "@composer push"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "enable-asset-packagist": "Acquia\\Lightning\\Composer\\AssetPackagist::execute",
        "nuke": "rm -r -f docroot vendor",
        "package": "Acquia\\Lightning\\Composer\\Package::execute",
        "pull": "cp -R -f ./docroot/profiles/contrib/lightning/* .",
        "verify-patched-constraints": "Acquia\\Lightning\\Composer\\PatchedConstraint::execute"
    }
}
