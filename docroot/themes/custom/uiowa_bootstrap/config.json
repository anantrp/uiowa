{
  "name": "uiowa_bootstrap",
  "description": "Uiowa Base theme",
  "css": {
    "file" : "uiowa_bootstrap.style.scss",
    "src": [
      "scss/**/*.scss"
    ],
    "dest": "assets/css",
    "includePaths": [
      "./node_modules"
    ]
  },
  "js": {
    "file" : [
      "{{machine_name}}.behavior.js",
      "bootstrap.bundle.js"
    ],
    "src": [
      "js/**/*.js",
      "../../../../node_modules/bootstrap/dist/js/bootstrap.bundle.js"
    ],
    "dest": "assets/js"
  }
}
