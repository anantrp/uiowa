# Ignore directories generated by Composer.
/drush/Commands
/drush/sites
/vendor/

# Ignore the entire docroot directory since it will be built with Pipelines.
/docroot/

# Ignore sensitive information
/docroot/sites/*/settings.php
/docroot/sites/*/settings.local.php

# Ignore Drupal's file directory
/docroot/sites/*/files/

# Ignore editor files.
.idea
.vscode

# Ignore OSX files
.DS_Store
